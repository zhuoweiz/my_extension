{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "description": "hahahahhahha",
  "version": "0.1",


  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.2.1.js","content.js"]
  }
  ],


  "browser_action": {
  "default_icon": "icon.png",
  "default_popup": "cool.html"
},

"permissions": [
    "activeTab",
    "storage"
],

"background": {
  "scripts": ["background.js"]
}


}
